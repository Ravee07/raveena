@model System.Data.DataTable
{

    ViewBag.Title = "ListDepartment";

}
<table class="table table-bordered table-striped">
    <tr>
        <th> DeptNo</th>
        
        <th> DeptName</th>
    </tr>

    @for (int i = 0; i < Model.Rows.Count; i++)
    {
        <tr>
            <!--<td>-->
                @*<a href="@Url.Action("Index", "Information", new { @id = @Model.Rows[i][0] })">*@ <!--@Model.Rows[i][0]-->@*</a>*@
            <!--</td>-->
            <td>@Model.Rows[i][0]</td>
            <td>@Model.Rows[i][1]</td>
        </tr>

    }
</table>
<a href=" @Url.Action("CreateDepartment","Departmentcontroller1")">Add Department</a>
