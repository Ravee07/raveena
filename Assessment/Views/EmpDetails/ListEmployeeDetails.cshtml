List of Employees

@model System.Data.DataTable
@{
    ViewBag.Title = "ListEmployee";

}
<table class="table table-bordered table-striped">
    <tr>
        <th> EmpDetailid</th>
        <th> EmpName</th>
        <th> DateOfbirth</th>
        <th> DateOfJoining</th>
        <th> Designation</th>
        <th> Degree</th>
        <th> Passoutyear</th>

    </tr>

    @for (int i = 0; i < Model.Rows.Count; i++)
    {
<tr>
    <td>
        @*<a href="@Url.Action("Index", "Information", new { @id = @Model.Rows[i][0] })">*@ @Model.Rows[i][0]@*</a>*@
    </td>
    <td>@Model.Rows[i][0]</td>
    <td>@Model.Rows[i][1]</td>
    <td>@Model.Rows[i][2]</td>
    <td>@Model.Rows[i][3]</td>
    <td>@Model.Rows[i][4]</td>
    <td>@Model.Rows[i][5]</td>
    <td>@Model.Rows[i][6]</td>
   
</tr>

    }
</table>
<a href=" @Url.Action("Create","newEmp")">Add Employee</a>