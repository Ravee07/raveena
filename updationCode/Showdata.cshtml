


@{
    ViewBag.Title = "Showdata";
}

<h2>Showdata</h2>

 <!DOCTYPE>
<html>
<head>
    <meta name=" viewport" contect="width=device-width" />
    <title> Show_data</title>
</head>
<body>

    <div class="table-responsive">
        <table class=" table table-striped table-bordered table-hover table-condensed">
            <thead>
                <tr>
                    <th>Empid</th>
                    <th>EmpName</th>
                    <th>Deptid</th>


                </tr>
            </thead>
            <tbody>
                @{
                    foreach (System.Data.DataRow dr in ViewBag.emp.Rows)



                    {


                        <tr>
                            <td>@dr["Empid"]</td>
                            <td>@dr["EmpName"]</td>
                            <td>@dr["Deptid"]</td>

                        </tr>
                    }
                }
            </tbody>







        </table>








    </div>



</body>


</html>


